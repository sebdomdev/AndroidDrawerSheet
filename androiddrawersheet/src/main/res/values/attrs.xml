<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AndroidDrawerSheet">
        <attr name="drawerOffset" format="dimension"/>
        <attr name="drawerAlignment" format="enum">
            <enum name="top" value="1"/>
            <enum name="bottom" value="2"/>
            <enum name="left" value="3"/>
            <enum name="right" value="4"/>
        </attr>
        <attr name="drawerOffsetInvisible" format="boolean"/>
        <attr name="minimumClosingSize" format="dimension"/>
        <attr name="minimumOpeningSize" format="dimension"/>
        <attr name="stickyDrag" format="boolean"/>
    </declare-styleable>
</resources>